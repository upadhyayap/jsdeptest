{
    "name": "endor-typescript",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "cp_bazel_libs": "bazel run //src/typescript:cp_bazel_libs",
        "cp_bazel_libs_maybe": "[ -d './node_modules/@endorlabs/api_client' ] || bazel run //src/typescript:cp_bazel_libs",
        "lint": "yarn run eslint ./",
        "storybook": "yarn run cp_bazel_libs && start-storybook -p 6006 -c packages/storybook/config --https --ssl-cert ../../infra/development/devcerts/localhost+2.pem --ssl-key ../../infra/development/devcerts/localhost+2-key.pem",
        "storybook:build": "build-storybook -c packages/storybook/config",
        "test": "yarn run cp_bazel_libs_maybe && jest --ci --colors --coverage=true",
        "webapp:build:dev": "webpack-cli build --mode development -c packages/webapp/webpack.config.js ",
        "webapp:build:prod": "webpack-cli build --mode production -c packages/webapp/webpack.config.js ",
        "webapp:dev": "yarn run cp_bazel_libs && webpack-cli serve -c packages/webapp/webpack.config.js --server-type https --server-options-key ../../infra/development/devcerts/localhost+2-key.pem --server-options-cert ../../infra/development/devcerts/localhost+2.pem",
        "webapp:i18n": "yarn webapp:i18n:extract && formatjs compile ./packages/webapp/locales/extracted/en.extracted.json --ast --out-file ./packages/webapp/locales/en.json",
        "webapp:i18n:extract": "formatjs extract './packages/webapp/**/*.ts*' --ignore 'node_modules' '**/*d.ts*' --out-file ./packages/webapp/locales/extracted/en.extracted.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --additional-component-names FM"
    },
